  "tokenColors": [
    // ============================================
    // COMMENTS
    // Muted gray - visible but not competing
    // WCAG AA contrast: 4.5:1 on #1C1E22
    // ============================================
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6B7A87",
        "fontStyle": "italic"
      }
    },

    // ============================================
    // STRINGS (Data family - Teal-green)
    // Shifted from pure green to teal-green
    // Colorblind-safe: distinguishable from red/orange
    // ============================================
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },
    {
      "name": "String escape characters",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#70D8DC"
      }
    },

    // ============================================
    // NUMBERS & CONSTANTS (Data family - Amber)
    // Warm amber - high luminance contrast with strings
    // ============================================
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // KEYWORDS & CONTROL FLOW (Structural - Purple)
    // Universally accessible, calming
    // ============================================
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B794D4"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#B794D4"
      }
    },

    // ============================================
    // OPERATORS (Neutral)
    // Low saturation, not distracting
    // ============================================
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#A1A8B3"
      }
    },

    // ============================================
    // FUNCTIONS (Callable - Blue)
    // Clear blue, distinct from purple and teal
    // ============================================
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#6AAFE6"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#A1A8B3"
      }
    },

    // ============================================
    // CLASSES & TYPES (Types - Teal)
    // Brighter than functions for luminance contrast
    // ============================================
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },
    {
      "name": "Type parameters (generics)",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#70D8DC"
      }
    },

    // ============================================
    // VARIABLES (Neutral)
    // Base readability, neutral gray
    // ============================================
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#D8DCE2"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#B8BCC4"
      }
    },
    {
      "name": "Constant variables",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // HTML/JSX TAGS (Markup - Terracotta)
    // NOT pure red - shifted toward orange
    // Colorblind-safe
    // ============================================
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#E8835A"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "JSX Component Tags",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },

    // ============================================
    // CSS
    // ============================================
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#6AAFE6"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // JSON
    // ============================================
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6AAFE6"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },

    // ============================================
    // MARKDOWN
    // ============================================
    {
      "name": "Markdown Headings",
      "scope": [
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#6AAFE6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D8DCE2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#D8DCE2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6B7A87",
        "fontStyle": "italic"
      }
    },

    // ============================================
    // PUNCTUATION
    // Subtle, reduced visual noise
    // ============================================
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#71717A"
      }
    },

    // ============================================
    // SUPPORT & LIBRARY
    // ============================================
    {
      "name": "Library/Framework",
      "scope": [
        "support.variable",
        "support.constant"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },

    // ============================================
    // IMPORTS/MODULES
    // ============================================
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#B794D4"
      }
    },
    {
      "name": "Module names",
      "scope": [
        "entity.name.import",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },

    // ============================================
    // DECORATORS/ANNOTATIONS
    // ============================================
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // INVALID/DEPRECATED
    // ============================================
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E8835A",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#E8A854",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#D8DCE2",
    "variable.readonly": "#E8A854",
    "variable.defaultLibrary": "#56C4C8",
    "property": "#B8BCC4",
    "property.readonly": "#E8A854",
    "function": "#6AAFE6",
    "function.defaultLibrary": "#6AAFE6",
