  "tokenColors": [
    // ============================================
    // COMMENTS
    // ============================================
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7F8A93",
        "fontStyle": "italic"
      }
    },

    // ============================================
    // STRINGS
    // Teal-green (colorblind-safe)
    // ============================================
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#1A7A6C"
      }
    },
    {
      "name": "String escape characters",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#1A7A6C"
      }
    },

    // ============================================
    // NUMBERS & CONSTANTS
    // Amber (colorblind-safe)
    // ============================================
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#B35D00"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#9E6B00"
      }
    },

    // ============================================
    // KEYWORDS & CONTROL FLOW
    // Purple (colorblind-safe)
    // ============================================
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7B5EA7"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#7B5EA7"
      }
    },

    // ============================================
    // OPERATORS
    // ============================================
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#6B5D4F"
      }
    },

    // ============================================
    // FUNCTIONS
    // Blue (colorblind-safe)
    // ============================================
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#2A6DB0"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#7A7068"
      }
    },

    // ============================================
    // CLASSES & TYPES
    // Teal (colorblind-safe)
    // ============================================
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#0D7377"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias"
      ],
      "settings": {
        "foreground": "#0D7377"
      }
    },
    {
      "name": "Type parameters (generics)",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#0D7377"
      }
    },

    // ============================================
    // VARIABLES
    // ============================================
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#433D38"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#6B5D4F"
      }
    },
    {
      "name": "Constant variables",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#B35D00"
      }
    },

    // ============================================
    // HTML/JSX TAGS
    // ============================================
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#C65D3D"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9E6B00"
      }
    },
    {
      "name": "JSX Component Tags",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#0D7377"
      }
    },

    // ============================================
    // CSS
    // ============================================
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#9E6B00"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#2A6DB0"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#1A7A6C"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#B35D00"
      }
    },

    // ============================================
    // JSON
    // ============================================
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#2A6DB0"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#1A7A6C"
      }
    },

    // ============================================
    // MARKDOWN
    // ============================================
    {
      "name": "Markdown Headings",
      "scope": [
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#2A6DB0",
        "fontStyle": "bold"
