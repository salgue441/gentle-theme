    "peekViewEditor.background": "#121010",
    "peekViewEditor.matchHighlightBackground": "#8A6A4040",
    "peekViewResult.background": "#0C0A08",
    "peekViewResult.fileForeground": "#D0C8C0",
    "peekViewResult.lineForeground": "#988878",
    "peekViewResult.matchHighlightBackground": "#8A6A4040",
    "peekViewResult.selectionBackground": "#4A3A28",
    "peekViewResult.selectionForeground": "#D0C8C0",
    "peekViewTitle.background": "#0A0808",
    "peekViewTitleDescription.foreground": "#988878",
    "peekViewTitleLabel.foreground": "#D0C8C0",

    // ============================================
    // MERGE CONFLICTS
    // ============================================
    "merge.currentHeaderBackground": "#70A07040",
    "merge.currentContentBackground": "#70A07020",
    "merge.incomingHeaderBackground": "#A8987040",
    "merge.incomingContentBackground": "#A8987020",

    // ============================================
    // PANEL
    // ============================================
    "panel.background": "#0C0A08",
    "panel.border": "#181410",
    "panelTitle.activeBorder": "#90A870",
    "panelTitle.activeForeground": "#D0C8C0",
    "panelTitle.inactiveForeground": "#685850",

    // ============================================
    // TERMINAL
    // ============================================
    "terminal.background": "#0C0A08",
    "terminal.foreground": "#D0C8C0",
    "terminal.ansiBlack": "#181410",
    "terminal.ansiRed": "#D89080",
    "terminal.ansiGreen": "#80B080",
    "terminal.ansiYellow": "#D8B060",
    "terminal.ansiBlue": "#98A8B8",
    "terminal.ansiMagenta": "#B898A8",
    "terminal.ansiCyan": "#80B8A8",
    "terminal.ansiWhite": "#D0C8C0",
    "terminal.ansiBrightBlack": "#3A3530",
    "terminal.ansiBrightRed": "#E8A898",
    "terminal.ansiBrightGreen": "#A0D0A0",
    "terminal.ansiBrightYellow": "#E8D088",
    "terminal.ansiBrightBlue": "#A8B8C8",
    "terminal.ansiBrightMagenta": "#C8A8B8",
    "terminal.ansiBrightCyan": "#A0D0C0",
    "terminal.ansiBrightWhite": "#E8E0D8",
    "terminalCursor.foreground": "#D0C8C0",

    // ============================================
    // NOTIFICATIONS
    // ============================================
    "notifications.background": "#121010",
    "notifications.foreground": "#D0C8C0",
    "notifications.border": "#181410",
    "notificationsErrorIcon.foreground": "#E8835A",
    "notificationsWarningIcon.foreground": "#E8A854",
    "notificationsInfoIcon.foreground": "#90A870",

    // ============================================
    // GIT DECORATIONS
    // ============================================
