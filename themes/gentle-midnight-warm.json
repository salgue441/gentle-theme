    "gitDecoration.untrackedResourceForeground": "#5FB49C",
    "gitDecoration.ignoredResourceForeground": "#3A3530",
    "gitDecoration.conflictingResourceForeground": "#E8A854",
    "gitDecoration.addedResourceForeground": "#5FB49C",
    "gitDecoration.stageModifiedResourceForeground": "#6AAFE6",
    "gitDecoration.stageDeletedResourceForeground": "#E8835A",

    // ============================================
    // BREADCRUMBS
    // ============================================
    "breadcrumb.background": "#100E0C",
    "breadcrumb.foreground": "#685850",
    "breadcrumb.focusForeground": "#D0C8C0",
    "breadcrumb.activeSelectionForeground": "#D0C8C0",
    "breadcrumbPicker.background": "#121010",

    // ============================================
    // EDITOR WIDGETS
    // ============================================
    "editorWidget.background": "#121010",
    "editorWidget.foreground": "#D0C8C0",
    "editorWidget.border": "#181410",
    "editorSuggestWidget.background": "#100E0C",
    "editorSuggestWidget.border": "#181410",
    "editorSuggestWidget.foreground": "#D0C8C0",
    "editorSuggestWidget.highlightForeground": "#C8B890",
    "editorSuggestWidget.selectedBackground": "#4A3A28",
    "editorHoverWidget.background": "#121010",
    "editorHoverWidget.border": "#181410",

    // ============================================
    // DEBUG
    // ============================================
    "debugToolBar.background": "#121010",
    "debugIcon.breakpointForeground": "#E8835A",
    "debugIcon.breakpointDisabledForeground": "#3A3530",
    "debugIcon.startForeground": "#5FB49C",
    "debugIcon.pauseForeground": "#E8A854",
    "debugIcon.stopForeground": "#E8835A",
    "debugIcon.restartForeground": "#5FB49C",
    "debugConsole.infoForeground": "#6AAFE6",
    "debugConsole.warningForeground": "#E8A854",
    "debugConsole.errorForeground": "#E8835A",

    // ============================================
    // TESTING
    // ============================================
    "testing.iconFailed": "#E8835A",
    "testing.iconErrored": "#E8835A",
    "testing.iconPassed": "#5FB49C",
    "testing.iconQueued": "#E8A854",
    "testing.iconUnset": "#3A3530",
    "testing.iconSkipped": "#3A3530"
  },
  "tokenColors": [
    // ============================================
    // COMMENTS
    // ============================================
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6B7A87",
        "fontStyle": "italic"
      }
    },

    // ============================================
    // STRINGS
    // Teal-green (colorblind-safe)
    // ============================================
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },
    {
      "name": "String escape characters",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },

    // ============================================
    // NUMBERS & CONSTANTS
    // Amber (colorblind-safe)
    // ============================================
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // KEYWORDS & CONTROL FLOW
    // Purple (colorblind-safe)
    // ============================================
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B794D4"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.return",
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#B794D4"
      }
    },

    // ============================================
    // OPERATORS
    // ============================================
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#988878"
      }
    },

    // ============================================
    // FUNCTIONS
    // Blue (colorblind-safe)
    // ============================================
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#6AAFE6"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#988878"
      }
    },

    // ============================================
    // CLASSES & TYPES
    // Teal (colorblind-safe)
    // ============================================
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },
    {
      "name": "Interfaces",
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },
    {
      "name": "Type parameters (generics)",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },

    // ============================================
    // VARIABLES
    // ============================================
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#D0C8C0"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#B0A8A0"
      }
    },
    {
      "name": "Constant variables",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // HTML/JSX TAGS
    // ============================================
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#E8835A"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "JSX Component Tags",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },

    // ============================================
    // CSS
    // ============================================
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#6AAFE6"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // JSON
    // ============================================
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6AAFE6"
      }
    },
    {
      "name": "JSON Values",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },

    // ============================================
    // MARKDOWN
    // ============================================
    {
      "name": "Markdown Headings",
      "scope": [
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#6AAFE6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#D0C8C0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#D0C8C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6B7A87",
        "fontStyle": "italic"
      }
    },

    // ============================================
    // PUNCTUATION
    // ============================================
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#686058"
      }
    },

    // ============================================
    // SUPPORT & LIBRARY
    // ============================================
    {
      "name": "Library/Framework",
      "scope": [
        "support.variable",
        "support.constant"
      ],
      "settings": {
        "foreground": "#56C4C8"
      }
    },

    // ============================================
    // IMPORTS/MODULES
    // ============================================
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#B794D4"
      }
    },
    {
      "name": "Module names",
      "scope": [
        "entity.name.import",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#5FB49C"
      }
    },

    // ============================================
    // DECORATORS/ANNOTATIONS
    // ============================================
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#E8A854"
      }
    },

    // ============================================
    // INVALID/DEPRECATED
    // ============================================
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E8835A",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#E8A854",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#D0C8C0",
    "variable.readonly": "#E8A854",
    "variable.defaultLibrary": "#56C4C8",
    "property": "#B0A8A0",
    "property.readonly": "#E8A854",
    "function": "#6AAFE6",
    "function.defaultLibrary": "#6AAFE6",
    "method": "#6AAFE6",
    "class": "#56C4C8",
    "class.defaultLibrary": "#56C4C8",
    "interface": "#56C4C8",
    "type": "#56C4C8",
    "type.defaultLibrary": "#56C4C8",
    "typeParameter": "#56C4C8",
    "namespace": "#56C4C8",
    "enum": "#56C4C8",
    "enumMember": "#E8A854",
    "parameter": "#988878",
    "keyword": "#B794D4",
    "modifier": "#B794D4",
    "comment": "#6B7A87",
    "string": "#5FB49C",
    "number": "#E8A854",
    "regexp": "#5FB49C",
    "operator": "#988878"
  }
}
